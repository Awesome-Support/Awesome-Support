{
    "name": "awesome-support/awesome-support",
    "description": "Awesome Support is the most versatile support plugin for WordPress",
    "type": "wordpress-plugin",
    "keywords": [
        "help",
        "helpdesk",
        "support",
        "support staff",
        "ticketing",
        "tickets"
    ],
    "homepage": "https://getawesomesupport.com",
    "license": "GNU GPL",
    "authors": [
        {
            "name": "Julien Liabeuf",
            "email": "julien@liabeuf.fr",
            "homepage": "http://julienliabeuf.com",
            "role": "Lead Developer"
        },
        {
            "name": "Julien Vernet",
            "email": "julien@vernet.me",
            "homepage": "http://siamkreative.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "gambitph/titan-framework": "1.9.*",
        "ericmann/wp-session-manager": "1.2.*"
    },
    "archive": {
        "exclude": [
            ".gitignore",
            "*.sublime-workspace",
            "*.sublime-project",
            "apigen.neon",
            "composer.json",
            "composer.lock",
            "README.md",
            "Gruntfile.js",
            "package.json",
            "/node_modules",
            "/tests",
            "!/vendor"
        ]
    },
    "scripts": {
        "post-install-cmd": "npm install",
        "pre-archive-cmd": "composer install"
    },
    "post-update-cmd": {
    }
}